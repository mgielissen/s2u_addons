<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inventroy_product_view" model="ir.ui.view">
        <field name="name">Inventory Product</field>
        <field name="model">report.s2u_warehouse.inventory.product</field>
        <field name="arch" type="xml">
            <form string="Inventory Product">
                <field name="company_id" invisible="1"/>
                <group>
                    <group>
                        <field name="date_till"/>
                    </group>
                    <group>
                    </group>
                </group>
                <footer>
                    <button name="print_report" string="Print" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="s2u_warehouse_inventory_product_view_action" model="ir.actions.act_window">
        <field name="name">Inventory Product</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">report.s2u_warehouse.inventory.product</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="inventroy_product_view"/>
        <field name="target">new</field>
    </record>

        <menuitem id="menu_iranus_s2u_inventory_product"
                  parent="menu_warehouse_reporting"
                  action="s2u_warehouse_inventory_product_view_action"
                  sequence="100" groups="s2uwarehouse.group_warehouse_manager" />
</odoo>