<openerp>
    <data>
        <record id="purchase_form" model="ir.ui.view">
            <field name="inherit_id" ref="s2usale.purchase_form"/>
            <field name="model">s2u.purchase</field>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page[@name='delivery']/group/group/field[@name='date_delivery']" position="after">
                    <field name="date_artwork" />
                </xpath>
            </field>
        </record>
    </data>
</openerp>